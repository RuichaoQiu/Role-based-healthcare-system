package edu.ncsu.csc.itrust.beans;
/**
 * A bean for holding information about death statistics(death code,description, and count)
 * @author bllevy2
 */
public class DeathStatsBean {
	
	private String code;
	private String description;
	private int count;
	/**
	 * Constructor
	 */
	public DeathStatsBean() {
		this.code = null;
		this.description = null;
		this.count = 0;
	}
	/** 
	 * getCode
	 * @return death code(ICD)
	 */
	public String getCode() {
		return code;
	}
	/**
	 * setCode
	 * @param code the code to set code to
	 */
	public void setCode(String code) {
		this.code = code;
	}
	/**
	 * getDescription
	 * @return the description of the death cause
	 */
	public String getDescription() {
		return description;
	}
	/**
	 * setDescription
	 * @param description the string to set description to
	 */
	public void setDescription(String description) {
		this.description = description;
	}
	/**
	 * getCount
	 * @return number of people with this type of death
	 */
	public int getCount() {
		return count;
	}
	/**
	 * setCount
	 * @param count number of people that died of this death
	 */
	public void setCount(int count) {
		this.count = count;
	}

}
